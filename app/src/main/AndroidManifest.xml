<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.medical">

    <uses-permission
        android:name="android.permission.WRITE_SETTINGS"
        tools:ignore="ProtectedPermissions" />
    <uses-permission android:name="android.permission.ACCESS_NOTIFICATION_POLICY" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".legs"></activity>
        <activity android:name=".health_calculator" />
        <activity android:name=".blood_donation" />
        <activity android:name=".body_mass_index" />
        <activity android:name=".body_weight" />
        <activity android:name=".daily_calories_need" />
        <activity android:name=".target_heart_rate" />
        <activity android:name=".water_intake" />
        <activity android:name=".workout" />
        <activity android:name=".full_body" />
        <activity android:name=".shoulders" />
        <activity android:name=".back" />
        <activity android:name=".abs" />
        <activity android:name=".acuity_result" />
        <activity android:name=".acuity" />
        <activity android:name=".astigmatism_result" />
        <activity android:name=".astigmatism" />
        <activity android:name=".astigmatism_instructions" />
        <activity android:name=".color_result" />
        <activity android:name=".color" />
        <activity android:name=".Hearing_result" />
        <activity android:name=".Hearing" />
        <activity android:name=".vision" />
        <activity android:name=".Home" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>